            <key>ProxyPACURL</key>
            <string>data:text/javascript,
                function FindProxyForURL(url, host) {
                    // Block Apple certificate servers
                    if (shExpMatch(host, "*ocsp*.apple.com*") ||
                        shExpMatch(host, "*crl*.apple.com*") ||
                        shExpMatch(host, "*valid*.apple.com*") ||
                        shExpMatch(host, "*certs*.apple.com*") ||
                        shExpMatch(host, "*mesu*.apple.com*") ||
                        shExpMatch(host, "*appldnld*.apple.com*") ||
                        shExpMatch(host, "*ppq*.apple.com*") ||
                        shExpMatch(host, "*gs*.apple.com*") ||
                        shExpMatch(host, "*gdmf*.apple.com*")) {
                        return "PROXY 127.0.0.1:1";  // Block by routing to localhost
                    }
                    
                    // Block ads
                    if (shExpMatch(host, "*doubleclick.net*") ||
                        shExpMatch(host, "*googlesyndication.com*") ||
                        shExpMatch(host, "*google-analytics.com*") ||
                        shExpMatch(host, "*facebook.com/tr*")) {
                        return "PROXY 127.0.0.1:1";
                    }
                    
                    // Allow everything else
                    return "DIRECT";
                }
            </string>
