            <string>data:text/javascript,function FindProxyForURL(url, host) {
    // Block Apple certificate revocation servers
    var blockedHosts = [
        'ocsp.apple.com',
        'crl.apple.com', 
        'valid.apple.com',
        'certs.apple.com',
        'ocsp2.apple.com',
        'world-gen.g.aaplimg.com',
