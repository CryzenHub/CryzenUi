        'appldnld.apple.com',
        'ppq.apple.com',
        'gs.apple.com',
        'gdmf.apple.com'
    ];
    
    // Check if current host should be blocked
    for (var i = 0; i < blockedHosts.length; i++) {
        if (shExpMatch(host, '*' + blockedHosts[i] + '*')) {
            return 'PROXY 127.0.0.1:1'; // Block by routing to localhost
        }
    }
    
    // Block common ad domains
    var adHosts = [
        'googleads.g.doubleclick.net',
        'pagead2.googlesyndication.com',
        'tpc.googlesyndication.com',
        'google-analytics.com',
        'googletagmanager.com'
    ];
    
    for (var j = 0; j < adHosts.length; j++) {
        if (shExpMatch(host, '*' + adHosts[j] + '*')) {
            return 'PROXY 127.0.0.1:1';
        }
    }
    
    // Allow everything else direct connection
    return 'DIRECT';
}</string>
